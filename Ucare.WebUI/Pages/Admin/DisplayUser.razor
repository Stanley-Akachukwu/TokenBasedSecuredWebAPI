@inherits DisplayUserBase

                 <tr>
                    <td>
                        @registeredUser.UserName
                    </td>
                    <td>
                     <button type="button" class="btn btn-primary"
                        @onclick="ShowUserEditor_Click">
                            Edit
                        </button>
                    </td>
                    <td>
                        <button type="button" class="btn btn-danger"
                        @onclick="Delete_Click">
                            Delete
                        </button>
                    </td>
                    <td><span class="label label-success">Approved</span></td>
                </tr>
           <Confirm ConfirmationChanged="ConfirmDelete_Click" @ref="DeleteConfirmation"
ConfirmationMessage=@($"Are you sure you want to delete \"{registeredUser.UserName}\"")></Confirm>

 
 
 